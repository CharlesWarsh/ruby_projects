<h1>Contacts Page</h1>

<h2><%= @contact.full_contact %></h2>
<h4><%= @contact.email_address %></h4>
<h4><%= @contact.jappy %></h4>
<h5><%= @contact.bio %></h5>
<h5><%= link_to "Delete", "/contacts/#{@contact.id}", method: :delete %> </h5>
<h5><%= link_to "Edit", "/contacts/#{@contact.id}/edit" %> </h5>
<br>
<h5>Last updated on: <%= @contact.readable_time %></h5>
<h5><%= link_to "All Contacts", "/contacts" %></h5>